{
    "color": [
        {
            "reg": "^(JS|TS) 堆栈:",
            "color": "white",
            "background": "red",
            "enable": true
        },
        {
            "reg": "\\[错误\\]",
            "color": "white",
            "background": "red",
            "enable": true
        },
        {
            "reg": "^\\s*at\\s*.*\\d\\)\\s*$",
            "color": "white",
            "background": "red",
            "enable": true
        },
        {
            "reg": "[^\\w]Error[^\\w]",
            "color": "red",
            "enable": true
        },
        {
            "reg": "[^\\w]Warning[^\\w]",
            "color": "yellow",
            "enable": true
        },
        {
            "reg": "\\[警告\\]",
            "color": "yellow",
            "enable": true
        },
        {
            "reg": "\\[Net\\]\\[ZQ\\]",
            "color": "gray",
            "enable": true
        }
    ],
    "replacing": [
        {
            "reg": "^\\[(\\d+.\\d+.\\d+-(\\d+)\\.(\\d+)\\.(\\d+):(\\d+))\\]\\[\\s*(\\d+)\\]",
            "replace": "[$2:$3:$4.$5($6帧)]",
            "enable": true
        },
        {
            "reg": "\\[GameThread\\].*\\[(信息|警告|错误)\\]",
            "replace": "[$1]",
            "enable": true
        },
        {
            "reg": "\\[(信息|警告|错误)\\]\\[(\\w+)\\](\\[[^\\[\\]]+\\])*",
            "replace": "[$1][$2]",
            "enable": true
        },
        {
            "reg": "D:\\\\aki\\\\Source\\\\Client",
            "replace": " ..项目路径",
            "enable": true
        },
        {
            "reg": "Puerts: \\(0x[0-9a-fA-F]+\\)?",
            "replace": "",
            "enable": true
        },
        {
            "reg": "\\[调试\\]\\[Net\\]\\[ZQ\\].* Net\\.\\w+\\.(\\w+)",
            "replace": "[协议]$1",
            "enable": false
        },
        {
            "reg": "\\[GameThread\\]",
            "replace": "",
            "enable": true
        },
        {
            "reg": "\"CombatCommon\":{\"PreMessageId\":.*,\"MessageId\":(.*),\"Originator\":.*,\"TimeStamp\":.*,\"EntityId\":.*,\"IsServerRequest\":(true|false)}",
            "replace": "",
            "enable": true
        },
        {
            "reg": "Net.(Notify|Response).(\\w+)",
            "replace": "$2",
            "enable": true
        }
    ],
    "filter": [
        {
            "reg": "\\[Net\\]\\[ZQ\\]",
            "exclude": false,
            "enable": true
        }
    ]
}